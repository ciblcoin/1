import { supabase } from '@/lib/supabaseClient' // مسیر فایل سرویس‌دهنده

// یک تابع تست برای چک کردن اتصال
async function testConnection() {
  const { data, error } = await supabase.from('profiles').select('*').limit(1)
  if(error) {
    console.error('خطا در اتصال به Supabase:', error)
  } else {
    console.log('اتصال موفق! داده‌ها:', data)
  }
}

// فراخوانی تابع در کد کامپوننت شما